{
  "name": "@origin/notifications",
  "version": "0.0.1",
  "description": "Origin Notifications Service",
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OriginProtocol/origin"
  },
